# set these env vars before running docker-compose up

# the host used by this repo-mon application
export GIT_HOST=

# the api host used by this repo-mon application
export GIT_API_HOST=

# GitHub client application id
export GH_BASIC_CLIENT_ID=

# GitHub client app secret
export GH_BASIC_CLIENT_SECRET=

# password for the mysql server
export MYSQL_ROOT_PASSWORD=

# encryption key for tokens
export TOKEN_ENCRYPTION_KEY=